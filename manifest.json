{
	"manifest_version": 3,
	"name":"Twishare to mkkey",
	"version":"0.0.2",
	"description":"Twitter のシェアボタンから mkkey.net にシェアすることができるようにします。",
	"icons":{
		"16":"icons/16.png", 
		"48":"icons/48.png",
		"128":"icons/128.png"
	},
	"content_scripts":[
		{
			"run_at":"document_start",
			"matches":["https://twitter.com/intent/tweet*","http://twitter.com/intent/tweet*"],
			"js":["script.js"]
		}
	],
	"author":"Alkappa/alpaca-honke , emtkmkk",
	"homepage_url":"https://github.com/emtkmkk/twishare-to-mkkey/"
}
